#: layout.cpp:602
msgid "invalid 'layout.pos.row'"
msgstr "неправильный 'layout.pos.row'"

#: layout.cpp:606
msgid "invalid 'layout.pos.col'"
msgstr "неправильный 'layout.pos.col'"

