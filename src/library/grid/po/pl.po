#: gpar.cpp:634
msgid "updateGContext must only be called after initGContext"
msgstr "updateGContext() musi być wywołany tylko po initGContext()"

#: grid.cpp:205
msgid "Turning clipping on or off within a (clipping) path is no honoured"
msgstr "Włączanie i wyłączanie przycinania w obrębie ścieżki (przycinania) nie jest honorowane"

#: grid.cpp:247
msgid "cannot clip to rotated viewport"
msgstr "nie można przyciąć do obróconego okienka ekranu"

#: grid.cpp:462
msgid "Clipping paths within a (clipping) path are not honoured"
msgstr "Ścieżki przycinające w ścieżce (przycinającej) nie są honorowane"

#: grid.cpp:481
msgid "Masks within a (clipping) path are not honoured"
msgstr "Maski znajdujące się w ścieżce (przycinającej) nie są honorowane"

#: grid.cpp:705 grid.cpp:883
#, c-format
msgid "Viewport '%s' was not found"
msgstr "Okno widoku '%s' nie zostało znalezione"

#: grid.cpp:915 grid.cpp:920 grid.cpp:1040 grid.cpp:1045
msgid "cannot pop the top-level viewport ('grid' and 'graphics' output mixed?)"
msgstr "nie można wyświetlić okienka ekranu na wierzchu (wyjścia 'grid' oraz 'graphics' pomieszały się?)"

#: grid.cpp:1246
msgid "attempt to plot on null device"
msgstr "próba rysowania na pustym urządzeniu"

#: grid.cpp:1716
msgid "there is no layout defined"
msgstr "nie określono układu"

#: grid.cpp:1938
msgid "polygon edge not found (zero-width or zero-height?)"
msgstr "nie znaleziono krawędzi wielokąta (zerowa szerokość lub zerowa wysokość?)"

#: grid.cpp:1947
msgid "polygon edge not found"
msgstr "nie znaleziono krawędzi wielokąta"

#: grid.cpp:2099
msgid "require at least two points to draw arrow"
msgstr "wymaga co najmniej dwu punktów, aby narysować strzałkę"

#: grid.cpp:2467
msgid "non-finite control point in Xspline"
msgstr "nieskończony punkt kontrolny w 'Xspline'"

#: grid.cpp:3433
msgid "non-finite x or y in graphics path"
msgstr "nieskończony 'x' lub 'y' w ścieżce grafiki"

#: grid.cpp:3482
msgid "Empty raster"
msgstr "Pusty raster"

#: grid.cpp:4023
msgid "use of negative pch with symbol font is invalid"
msgstr "użycie ujemnej wartości 'pch' z symbolem jest niepoprawne"

#: grid.cpp:4029 grid.cpp:4085
msgid "Coordinates for text pch not yet supported"
msgstr "Współrzędne dla tekstowych pch nie są jeszcze obsługiwane"

#: grid.cpp:4090
#, c-format
msgid "pch value '%d' is invalid in this locale"
msgstr "wartość pch '%d' nie jest poprawna dla tej lokalizacji"

#: grid.cpp:4911
#, c-format
msgid "unimplemented pch value '%d'"
msgstr "niezaimplementowana wartość 'pch' '%d'"

#: grid.cpp:5069
msgid "invalid plotting symbol"
msgstr "niepoprawny symbol wykresu"

#: grid.cpp:5187
msgid "unable to clip to rotated rectangle"
msgstr "nie można przypiąć do obróconego prostokąta"

#: layout.cpp:602
msgid "invalid 'layout.pos.row'"
msgstr "niepoprawny 'layout.pos.row'"

#: layout.cpp:606
msgid "invalid 'layout.pos.col'"
msgstr "niepoprawny 'layout.pos.col'"

#: matrix.cpp:50
msgid "singular transformation matrix"
msgstr "osobliwa macierz transformacyjna"

#: state.cpp:197
msgid "unable to store 'grid' state.  Too many devices open?"
msgstr "nie można zachować stanu siatki. Zbyt duża liczba otwartych urządzeń?"

#: unit.cpp:76
msgid "Cannot create unit scalar from 0-length unit vector"
msgstr "Nie można utworzyć jednostkowego skalara z jednostkowego wektora o długości 0"

#: unit.cpp:806 unit.cpp:1315
msgid "invalid unit or unit not yet implemented"
msgstr "niedozwolona jednostka lub jednostka nie została jeszcze zaimplementowana"

#: unit.cpp:1268 unit.cpp:1372 unit.cpp:1403
msgid "Viewport has zero dimension(s)"
msgstr "Okno ekranu ma zerowe wymiary"

#: unit.cpp:1435 unit.cpp:1450 unit.cpp:1465 unit.cpp:1480
msgid "Unsupported unit conversion"
msgstr "Niewspierana jednostka konwersji"

#: unit.cpp:1572
msgid "Invalid unit"
msgstr "Niepoprawna jednostka"

#: unit.cpp:1587
msgid "'units' must be character"
msgstr "'units' muszą być tekstem"

#: unit.cpp:1590
#, c-format
msgid "'%s' must be of length > 0"
msgstr "'%s' muszą mieć długość > 0"

#: unit.cpp:1601
msgid "data must be either NULL, have length 1, or match the length of the final unit vector"
msgstr "dane muszą mieć wartość NULL, mieć długość 1 lub odpowiadać długości końcowego wektora jednostkowego"

#: unit.cpp:1613
msgid "no string supplied for 'strwidth/height' unit"
msgstr "nie podano ciągu znaków dla jednostki 'strwidth/height'"

#: unit.cpp:1617
msgid "no 'grob' supplied for 'grobwidth/height' unit"
msgstr "nie podano 'grob' dla jednostki 'grobwidth/height'"

#: unit.cpp:1644
msgid "'gPath' must have depth 1 in 'grobwidth/height' units"
msgstr "'gPath' musi mieć głębokość 1 w jednostkach 'grobwidth/height'"

#: unit.cpp:1649
msgid "non-NULL value supplied for plain unit"
msgstr "wartość różna od NULL podana dla zwykłej jednostki"

#: unit.cpp:1702 unit.cpp:1736 unit.cpp:1785
msgid "old version of unit class is no longer allowed"
msgstr "stara wersja klasy jednostek nie jest już dozwolona"

#: unit.cpp:1708
msgid "object is not coercible to a unit"
msgstr "obiekt nie jest przekształcalny do jednostki"

#: unit.cpp:1734
msgid "object is not a unit"
msgstr "obiekt nie jest jednostką"

#: unit.cpp:1869
msgid "units can only be multiplied with numerics and integers"
msgstr "jednostki można mnożyć tylko przez liczby i liczby całkowite"

#: util.cpp:220
msgid "Unable to calculate text width/height (using zero)"
msgstr "Nie można obliczyć szerokości/wysokości tekstu (używanie zera)"

#: util.cpp:308 util.cpp:320
msgid "grid grob object is empty"
msgstr "obiekt \"grob\" jest pusty"

#: viewport.cpp:59
msgid "Mask is not logical value ('none' or 'inherit')"
msgstr "Maska nie jest wartością logiczną ('none' lub 'inherit')"

#: viewport.cpp:333
msgid "non-finite location and/or size for viewport"
msgstr "nieskończone położenie oraz/lub rozmiar okna ekranu"

#~ msgid "unimplemented unit function"
#~ msgstr "niezaimplementowana jednostkowa funkcja"
