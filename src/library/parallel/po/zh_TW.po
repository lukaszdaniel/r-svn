#: fork.cpp:225
#, c-format
msgid "unable to terminate child process: %s"
msgstr "無法終止子行程：%s"

#: fork.cpp:252
#, c-format
msgid "unable to terminate child: %s"
msgstr "無法終止子行程：%s"

#: fork.cpp:331 fork.cpp:617 ncpus.cpp:90 ncpus.cpp:126 ncpus.cpp:181
msgid "memory allocation error"
msgstr "記憶體指派錯誤"

#: fork.cpp:387
#, c-format
msgid "invalid '%s' argument"
msgstr "無效的 '%s' 引數"

#: fork.cpp:448
msgid "Error while shutting down parallel: unable to terminate some child processes\n"
msgstr "當停止平行時發生錯誤：無法決定某些子行程\n"

#: fork.cpp:541 fork.cpp:544
msgid "unable to create a pipe"
msgstr "無法建立管道"

#: fork.cpp:566
#, c-format
msgid "unable to fork, possible reason: %s"
msgstr "無法分叉，可能的原因是：%s"

#: fork.cpp:758
msgid "only children can send data to the master process"
msgstr "只有子行程才能給主行程傳送資料"

#: fork.cpp:760
msgid "there is no pipe to the master process"
msgstr "有沒可以接到主行程的管道"

#: fork.cpp:762
msgid "content to send must be RAW, use serialize() if needed"
msgstr "傳送的內容必須是原始的，如果需要的話請用 serialize()"

#: fork.cpp:771 fork.cpp:782 fork.cpp:1201
msgid "write error, closing pipe to the master"
msgstr "寫入錯誤，請關閉通往主行程的管道"

#: fork.cpp:792
msgid "only the master process can send data to a child process"
msgstr "只有主行程才能給子行程傳送資料"

#: fork.cpp:800
#, c-format
msgid "child %d does not exist"
msgstr "不存在 %d 這個子行程"

#: fork.cpp:806
msgid "write error"
msgstr "寫入錯誤"

#: fork.cpp:894
#, c-format
msgid "cannot wait for child %d as it does not exist"
msgstr "無法等待子行程 %d 因為它不存在"

#: fork.cpp:953 fork.cpp:1080
#, c-format
msgid "error '%s' in select"
msgstr "選擇時産生了 '%s'"

#: fork.cpp:1170
msgid "'mckill' failed"
msgstr "'mckill' 失敗"

#: fork.cpp:1183
msgid "'mcexit' can only be used in a child process"
msgstr "'mcexit' 只能用於子行程"

#: fork.cpp:1215
msgid "'mcexit' failed"
msgstr "'mcexit' 失敗"

#: fork.cpp:1244 fork.cpp:1253
msgid "invalid CPU affinity specification"
msgstr "中央處理器親和度規格無效"

#: fork.cpp:1264
msgid "requested CPU set is too large for this system"
msgstr "所需要的中央處理器設定對於這個系統來說太大了"

#: fork.cpp:1287
msgid "retrieving CPU affinity set failed"
msgstr "無法取得中央處理器親和度"
