msgid "Scrolling.\n"
msgstr "Rulling.\n"

#: src/gnuwin32/console.cpp:2451
msgid "  Keyboard: PgUp, PgDown, Ctrl+Arrows, Ctrl+Home, Ctrl+End,\n"
msgstr "  Tastatur: Page Up, Page Down, Ctrl + piltastar, Ctrl + Home, Ctrl + End,\n"

