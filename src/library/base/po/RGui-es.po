msgid "Scrolling.\n"
msgstr "Desplazarse.\n"

#: src/gnuwin32/console.cpp:2451
msgid "  Keyboard: PgUp, PgDown, Ctrl+Arrows, Ctrl+Home, Ctrl+End,\n"
msgstr "  Teclado: PgUp, PgDown, Ctrl+Arrows, Ctrl+Home, Ctrl+End,\n"

